{
  "name": "dungeon-map",
  "version": "0.1.0",
  "description": "A React web applicaton for mapping old videogames.",
  "author": "Steve Cochrane <steve@stevecochrane.com>",
  "license": "ISC",
  "repository": {
    "type": "git",
    "url": "https://github.com/stevecochrane/dungeon-map.git"
  },
  "main": "dungeon-map.js",
  "scripts": {
    "build": "env NODE_ENV=production webpack --config webpack/production.js",
    "lint:js": "eslint --ignore-path .eslintignore --cache .",
    "server": "node dungeon-map.js",
    "start": "webpack-dev-server --config webpack/development.js",
    "test": "jest --coverage",
    "watch": "webpack --config webpack/development.js --progress --watch"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.js": [
      "eslint",
      "jest --bail --coverage --findRelatedTests"
    ],
    "*.{js,css,html}": "prettier --write"
  },
  "devDependencies": {
    "@babel/core": "7.10.2",
    "@babel/preset-env": "7.10.2",
    "@babel/preset-react": "7.10.1",
    "@testing-library/jest-dom": "5.1.1",
    "@testing-library/react": "10.0.1",
    "babel-loader": "8.0.5",
    "clean-webpack-plugin": "1.0.1",
    "core-js": "3.6.3",
    "css-loader": "2.1.0",
    "cssnano": "4.1.10",
    "eslint": "5.13.0",
    "eslint-loader": "2.1.2",
    "eslint-plugin-jest": "23.8.2",
    "eslint-plugin-react": "7.12.4",
    "html-webpack-plugin": "3.2.0",
    "husky": "4.2.3",
    "jest": "25.1.0",
    "lint-staged": "10.0.8",
    "mini-css-extract-plugin": "0.5.0",
    "optimize-css-assets-webpack-plugin": "5.0.1",
    "postcss-import": "12.0.1",
    "postcss-loader": "3.0.0",
    "postcss-preset-env": "6.7.0",
    "prettier": "1.19.1",
    "prop-types": "15.7.1",
    "react": "16.12.0",
    "react-dom": "16.12.0",
    "react-redux": "6.0.1",
    "redux": "4.0.1",
    "redux-devtools-extension": "2.13.8",
    "style-loader": "0.23.1",
    "tailwindcss": "1.6.0",
    "terser-webpack-plugin": "2.3.1",
    "webpack": "4.43.0",
    "webpack-cli": "3.3.11",
    "webpack-dev-server": "3.11.0",
    "webpack-merge": "4.2.1"
  },
  "dependencies": {
    "compression": "1.7.3",
    "express": "4.16.4"
  }
}
